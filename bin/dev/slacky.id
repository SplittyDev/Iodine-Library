use * from core

func main () {
        number = Int (input("plz insert number one: "))
        operation = Int (input("1 = +, 2 = -. 3 = *, 4 = /. Insert number pls for operation: "))
        number2 = Int (input("pls insert number 2 m8: "))
        if(operation == 1){
                answer = number + number2
                printf("Expression: {} + {} = ?. Answer: {}", number, number2, answer)
        }
        if(operation == 2){
                answer = number - number2
                printf("Expression: {} - {} = ?. Answer: {}", number, number2, answer)
        }
        if(operation == 3){
                answer = number * number2
                printf("Expression: {} * {} = ?. Answer: {}", number, number2, answer)
        }
        if(operation == 4){
                answer = number / number2
                printf("Expression: {} / {} = ?. Answer: {}", number, number2, answer)
        }

}
